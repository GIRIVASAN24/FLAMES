#include <bits/stdc++.h>
using namespace std;

int main() {
	string a,b;
	int c;
	getline(cin,a);
	getline(cin,b);
	int count=0;
	for(int i=0;i<a.size();i++){
	    for(int j=0;j<b.size();j++){
	        if(a[i]==b[i]){
	            a[i]='*';
	            b[j]='*';
	        }
	    }
	}
	cout<<a<<endl;
	cout<<b<<endl;
	for(int i=0;i<a.size();i++){
	    if(a[i]!='*'){
	        count++;
	    }
	}
	for(int i=0;i<b.size();i++){
	    if(b[i]==' ')
	    {
	        continue;
	    }
	    if(b[i]!='*'){
	        count++;
	    }
	}
	cout<<count<<endl;
	vector<char> flames = {'F','L','A','M','E','S'};
	int i=0;
	while(flames.size() > 1)
	{
	    i = (i+count-1)%flames.size();
	    flames.erase(flames.begin()+i);
	}
	cout << flames[0] << " ";
	
	
}
